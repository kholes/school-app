<!DOCTYPE html>
<html>
<head>
	<title></title>
</head>
<body>
<ul>
<li><a href="/">Home</a></li>
<li><a href="/teachers">Teachers</a></li>
<li><a href="/students">Students</a></li>
<li><a href="/subjects">Subjects</a></li>
</ul>
<h3>Students</h3>
<p><a href="/students/add">Add Students</a>
<table>
	<th>ID</th>
	<th>First Name</th>
	<th>Last Name</th>
	<th>Full Name</th>
	<th>Email</th>
	<th colspan="2">Action</th>
	<% data.forEach(i => {%>
	<tr>
		<td><%-i.id%></td>
		<td><%-i.last_name%></td>
		<td><%-i.first_name%></td>
		<td><%-i.getFullname()%></td>
		<td><%-i.email%></td>
		<td><a href="/students/edit/<%-i.id%>">Edit</a></td>
		<td><a href="/students/delete/<%-i.id%>">Delete</a></td>
	</tr>
	<%}) %>
</table>
</body>
</html>